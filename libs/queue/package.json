{
  "name": "@coffret/queue",
  "version": "0.0.1",
  "description": "Lightweight, type-safe, and memory-efficient queue for resource-sensitive applications.",
  "type": "module",
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist node_modules",
    "test": "ava"
  },
  "keywords": [
    "queue",
    "data-structure",
    "linked list",
    "fifo",
    "enqueue",
    "dequeue",
    "algorithm",
    "type-safe",
    "memory-efficient"
  ],
  "author": "Aadil Mehraj",
  "license": "MIT",
  "ava": {
    "files": [
      "./**/*.test.{js,ts}"
    ],
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "ava": "^6.1.1",
    "queue": "^7.0.0",
    "tsimp": "^2.0.10",
    "tsup": "*",
    "typescript": "*"
  }
}
